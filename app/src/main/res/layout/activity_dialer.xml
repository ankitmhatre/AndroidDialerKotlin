<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Dialer">


    <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:id="@+id/call_list_rv"
            android:onClick="buttonClickEvent"
            android:layout_height="match_parent"/>


    <include android:layout_marginBottom="84dp" layout="@layout/dialer_number_layout"
             android:layout_width="match_parent" android:id="@+id/dialerView"
             android:layout_height="wrap_content" android:layout_alignParentBottom="true"/>



</RelativeLayout>